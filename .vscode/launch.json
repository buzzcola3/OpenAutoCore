{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug autoapp",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bazel-bin/autoapp_bazel",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "${workspaceFolder}/runnec-gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "bazel-build-autoapp",
      "externalConsole": false
    }
  ]
}
