{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-autoapp",
      "type": "shell",
      "command": "cmake",
      "args": [
        "--build",
        "build",
        "-j"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    },
    {
      "label": "bazel-build-autoapp",
      "type": "shell",
      "command": "bazel",
      "args": [
        "build",
        "-c",
        "dbg",
        "//:autoapp_bazel"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ]
}
